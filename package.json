{
  "name": "react-isomorphic-static-site-generator-boilerplate",
  "version": "1.0.0",
  "description": "Yet another isomorphic react boilerplate. This one does not require node on server.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf public/*",
    "static": "cp static/* public/react-isomorphic-static-site-generator-boilerplate/",
    "compile": "node ./node_modules/webpack/bin/webpack.js --progress --stats --display-error-details -p",
    "build": "npm run static && npm run compile",
    "server": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --display-error-details --content-base public/",
    "start": "npm run build && npm run server"
  },
  "author": "Vaclav Synacek",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "babel": "5.8.23",
    "babel-core": "5.8.25",
    "babel-eslint": "4.1.3",
    "babel-loader": "5.3.2",
    "babel-runtime": "5.8.25",
    "eslint": "1.7.2",
    "eslint-friendly-formatter": "1.2.2",
    "eslint-loader": "1.1.0",
    "eslint-plugin-react": "3.6.3",
    "history": "1.12.5",
    "react": "0.14.0",
    "react-bootstrap": "0.27.2",
    "react-document-meta": "2.0.0-rc2",
    "react-dom": "0.14.0",
    "react-router": "1.0.0-rc3",
    "react-router-bootstrap": "0.19.2",
    "static-site-generator-webpack-plugin": "1.2.0",
    "webpack": "1.12.2",
    "webpack-dev-server": "1.12.1"
  },
  "dependencies": {}
}
