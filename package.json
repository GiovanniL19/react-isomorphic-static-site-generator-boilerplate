{
  "name": "react-isomorphic-static-site-generator-boilerplate",
  "version": "1.0.0",
  "description": "React static site generator boilerplate",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf public/*",
    "compile": "node ./node_modules/webpack/bin/webpack.js --display-error-details --optimize-minimize",
    "build": "yarn compile",
    "serve": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "start": "yarn build && yarn serve"
  },
  "author": "Vaclav Synacek",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "babel": "6.23.0",
    "babel-cli": "^6.24.0",
    "babel-core": "6.24.0",
    "babel-eslint": "7.2.1",
    "babel-loader": "6.4.1",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-runtime": "6.23.0",
    "eslint": "3.19.0",
    "eslint-friendly-formatter": "2.0.7",
    "eslint-loader": "1.7.1",
    "eslint-plugin-react": "6.10.3",
    "history": "3.2.1",
    "react": "15.4.2",
    "react-document-meta": "2.1.1",
    "react-dom": "15.4.2",
    "react-router": "3.0.0",
    "react-router-scroll": "^0.4.4",
    "static-site-generator-webpack-plugin": "3.4.1",
    "webpack": "2.3.3",
    "webpack-dev-server": "2.4.2"
  },
  "dependencies": {}
}
